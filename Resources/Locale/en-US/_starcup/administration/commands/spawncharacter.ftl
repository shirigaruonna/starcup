cmd-spawncharacter-desc = Spawns your currently selected/specified character
cmd-spawncharacter-help = Usage: spawncharacter [characterName] [job] [applyTraits] [transferMind]

cmd-spawncharacter-arg-character = [character] (default: Current)
cmd-spawncharacter-error-character = Unable to find profile for name "{$name}"

cmd-spawncharacter-arg-job = [job id] (default: Passenger)
cmd-spawncharacter-error-job = Job is invalid.

cmd-spawncharacter-arg-traits = [apply traits] (default: False)
cmd-spawncharacter-arg-transfermind = [transfer mind] (default: True)

cmd-spawncharacter-complete = Character spawned.
